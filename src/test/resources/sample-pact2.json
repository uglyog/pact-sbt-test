{
  "consumer": {
    "name": "Foo Web Client"
  },
  "provider": {
    "name": "sampleconsumer2"
  },
  "interactions": [
    {
      "description": "a post to query",
      "request": {
        "method": "post",
        "path": "/query",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "activities": [
            { "name": "Bob", "description": "Description of bob" },
            { "name": "Fred", "description": "Description of Fred" }
          ]
        }
      }
    }
  ],
  "metadata": {
    "pactSpecificationVersion": "2.0.0"
  }
}
